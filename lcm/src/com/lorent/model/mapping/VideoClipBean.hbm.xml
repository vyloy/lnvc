<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lorent.model">
	<class name="VideoClipBean" table="videoclip">
		<id name="id">
			<column name="id" />
			<generator class="identity" />
		</id>
		<!--  
		<property name="videoClipUrlHigh" column="videoclipurlhigh" />
		<property name="videoClipUrlStandard" column="videoclipurlstandard" />
		-->
		<property name="httpVideoUrlHyper" column="httpvideourlhyper" />
		<property name="rtspVideoUrlHyper" column="rtspvideourlhyper" />
		<property name="httpVideoUrlHigh" column="httpvideourlhigh" />
		<property name="httpVideoUrlStandard" column="httpvideourlstandard" />
		<property name="rtspVideoUrlHigh" column="rtspvideourlhigh" />
		<property name="rtspVideoUrlStandard" column="rtspvideourlstandard" />
		<property name="thumbnailUrl" column="thumbnailurl" />
		<property name="title" column="title" />
		<property name="description" column="description" />
		<property name="category" column="category" />
		<property name="createrName" column="creatername" />
		<property name="createrNo" column="createrno" />
		<property name="ismonitor" column="ismonitor" />
		<property name="duration" column="duration" />
		<property name="hits" column="hits" />
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.lorent.model">
	<class name="MonitorNetBean" table="NetTable" dynamic-insert="true" dynamic-update="true">
		<id name="id"> 
			<column name="NetID" />
			<generator class="identity" />
		</id>
		<property name="ipAddr" column="IPAddr" />
		<property name="port" column="Port" />
		<property name="channelid" column="ChannelID" />
	</class>
</hibernate-mapping>
